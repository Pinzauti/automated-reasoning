HELP = python main.py -h
ASP-MANUAL = python main.py asp manual
ASP-RANDOM = python main.py asp random
MINIZINC = python main.py minizinc

default:
	@echo Hello there, if you need any help just write make help

help:
	${HELP}

asp-manual:
	${ASP-MANUAL}

asp-random:
	${ASP-RANDOM}

minizinc:
	${MINIZINC}